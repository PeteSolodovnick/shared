<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="956.0" prefWidth="1207.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="address.mains.RootLayoutController">
